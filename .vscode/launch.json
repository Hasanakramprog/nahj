{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Debug explanation_scraper.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/explanation_scraper.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "stopOnEntry": false,
            "cwd": "${workspaceFolder}",
            "python": "${workspaceFolder}/.venv/Scripts/python.exe"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "stopOnEntry": false
        }
    ]
}
